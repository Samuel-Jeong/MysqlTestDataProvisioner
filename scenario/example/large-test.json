{
  "name": "Large Test Dataset",
  "description": "대량 테스트 데이터 - 성능 테스트 및 부하 테스트용",
  "tables": {
    "customers": {
      "count": 100
    },
    "categories": {
      "count": 20
    },
    "products": {
      "count": 500,
      "relations": {
        "category_id": "categories.id"
      }
    },
    "orders": {
      "count": 300,
      "relations": {
        "customer_id": "customers.id"
      }
    },
    "order_items": {
      "count": 1000,
      "relations": {
        "order_id": "orders.id",
        "product_id": "products.id"
      }
    },
    "reviews": {
      "count": 400,
      "relations": {
        "product_id": "products.id",
        "customer_id": "customers.id"
      }
    }
  }
}

